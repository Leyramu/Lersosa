<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <!--      模块版本号      -->
    <modelVersion>4.0.0</modelVersion>

    <!--      父工程      -->
    <parent>
        <groupId>leyramu.framework</groupId>
        <artifactId>lersosa-example</artifactId>
        <version>${revision}</version>
    </parent>

    <!--      模块描述      -->
    <description>演示模块</description>

    <!--      工件 ID      -->
    <artifactId>lersosa-example-demo</artifactId>

    <!--      依赖      -->
    <dependencies>
        <!--      Lersosa Common Nacos      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-nacos</artifactId>
        </dependency>

        <!--      Lersosa Common Sentinel      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-sentinel</artifactId>
        </dependency>

        <!--      Lersosa Common Log      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-log</artifactId>
        </dependency>

        <!--      Lersosa Common Doc      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-doc</artifactId>
        </dependency>

        <!--      Lersosa Common Security      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-security</artifactId>
        </dependency>

        <!--      Lersosa Common Web      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-web</artifactId>
        </dependency>

        <!--      Lersosa Common Mybatis      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-mybatis</artifactId>
        </dependency>

        <!--      Lersosa Common Dubbo      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-dubbo</artifactId>
        </dependency>

        <!--      Lersosa Common Idempotent      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-idempotent</artifactId>
        </dependency>

        <!--      Lersosa Common Mail      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-mail</artifactId>
        </dependency>

        <!--      Lersosa Common Sms      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-sms</artifactId>
        </dependency>

        <!--      Lersosa Common Encrypt      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-encrypt</artifactId>
        </dependency>

        <!--      Lersosa Common Tenant      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-tenant</artifactId>
        </dependency>

        <!--      Lersosa Common Elasticsearch      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-elasticsearch</artifactId>
        </dependency>

        <!--      Lersosa Common Sensitive      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-common-sensitive</artifactId>
        </dependency>

        <!--      Lersosa Api Workflow      -->
        <dependency>
            <groupId>leyramu.framework</groupId>
            <artifactId>lersosa-api-workflow</artifactId>
        </dependency>

        <!--      Spring Boot Starter Test      -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <!--      构建插件      -->
    <build>
        <finalName>${project.artifactId}</finalName>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <version>${spring-boot.version}</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>repackage</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
</project>
